{
  "name": "sidstack",
  "version": "0.4.1",
  "private": true,
  "type": "module",
  "description": "SidStack - Lightweight AI Agent Orchestration Platform",
  "author": "SidStack Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/junixlabs/sidstack.git"
  },
  "homepage": "https://sidstack.dev",
  "bugs": {
    "url": "https://github.com/junixlabs/sidstack/issues"
  },
  "keywords": [
    "ai",
    "claude",
    "mcp",
    "task-management",
    "knowledge-base",
    "impact-analysis",
    "tauri",
    "agent",
    "governance",
    "developer-tools"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "packages:build": "turbo run build --filter='./packages/*'",
    "packages:dev": "turbo run dev --filter='./packages/*' --parallel",
    "packages:test": "turbo run test --filter='./packages/*'"
  },
