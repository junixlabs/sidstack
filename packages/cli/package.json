{
  "name": "@sidstack/cli",
  "version": "0.4.2",
  "description": "SidStack CLI - Project Intelligence for AI Coding Agents",
  "author": "SidStack Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "ai",
    "agent",
    "orchestration",
    "sidstack",
    "claude",
    "oclif"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "sidstack": "./bin/run.js"
  },
  "scripts": {
    "build": "tsc && cp -r skills dist/ && cp -r templates dist/",
    "dev": "tsc --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.0.2",
    "@oclif/core": "^3.26.0",
    "@oclif/plugin-help": "^6.0.0",
    "@sidstack/shared": "workspace:*",
    "chalk": "^5.3.0",
    "glob": "^11.0.0",
    "ink": "^3.2.0",
    "ink-spinner": "^4.0.3",
    "ink-text-input": "^4.0.3",
    "react": "^17.0.2",
    "ws": "^8.18.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@oclif/test": "^3.2.0",
    "@testing-library/react": "^12.0.0",
    "@types/glob": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^17.0.0",
    "@types/ws": "^8.5.10",
    "ink-testing-library": "^2.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.4.0"
  },
  "oclif": {
    "bin": "sidstack",
    "dirname": "sidstack",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "topics": {
      "knowledge": {
        "description": "Manage knowledge documentation"
      },
      "governance": {
        "description": "View and check governance rules"
      },
      "preset": {
        "description": "Manage project presets"
      },
      "skill": {
        "description": "Manage agent skills"
      }
    }
  }
}
